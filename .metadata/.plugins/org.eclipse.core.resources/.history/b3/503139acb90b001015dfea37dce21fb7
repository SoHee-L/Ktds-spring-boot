# JDBC 설정
logging:
  level:
    root: info

spring:
  mvc:
    view:
      prefix: /WEB-INF/views/
      suffix: .jsp
  application:
    name: hello-spring
  datasource:
    url: jdbc:oracle:thin:@localhost:1521:XE
    driver-class-name: oracle.jdbc.driver.OracleDriver
    username: SPRING_BOOT
    password: 1234
mybatis:
  config-location: classpath:mybatis/mybatis-config.xml
	  mapper-locations: 
      classpath: com/ktdsuniversity/edu/lsh/bbs/dao/mapper/BoardDaoMapper.xml
